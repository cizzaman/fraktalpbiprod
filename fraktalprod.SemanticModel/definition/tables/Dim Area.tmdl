	/// Område Farge (Region Color)
	/// Assigns consistent color for each region for visualizations
	column 'Område Farge' =
			SWITCH(
				'Dim Area'[omraade_kode],
				"NO1", "#118DFF",  // Blue - Øst-Norge
				"NO2", "#E66C37",  // Orange - Sør-Norge
				"NO3", "#1AAB40",  // Green - Midt-Norge
				"NO4", "#744EC2",  // Purple - Nord-Norge
				"NO5", "#D9B300",  // Yellow - Vest-Norge
				"#CCCCCC"          // Gray - Default
			)
		dataType: string
		lineageTag: cc_omraadefarge
		summarizeBy: none

	/// Sort Order
	/// Ensures consistent NO1-NO5 ordering in visuals
	column 'Sort Order' =
			SWITCH(
				'Dim Area'[omraade_kode],
				"NO1", 1,
				"NO2", 2,
				"NO3", 3,
				"NO4", 4,
				"NO5", 5,
				99
			)
		dataType: int64
		isHidden
		formatString: 0
		lineageTag: cc_sortorder
		summarizeBy: sum

		annotation SummarizationSetBy = Automatic

